{
  "name": "dragdropsnubes",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|jpg|png|svg)$": "<rootDir>/empty-module.js",
      "^.+\\.css$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "ios.js"
